{
  "name": "streamvault-backend-server",
  "version": "1.0.0",
  "description": "Core Backend Api for streamvault application",
  "main": "index.js",
  "engines": {
    "node": "18.17.0",
    "npm": "9.8.1"
  },
  "config": {
    "imageRepo": "streamvault/api",
    "imageName": "streamvault-ctr-api",
    "imagePort": "3000",
    "internalContainerPort": "3000"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.src.json",
    "postbuild": "npm run copy:assets",
    "copy:assets": "copyfiles -f \"src/public/**/*\" \"dist/public/\"",
    "predocker:build": "echo npm_test",
    "docker:build": "cross-conf-env docker image build . -t $npm_package_config_imageRepo:$npm_package_version",
    "postdocker:build": "npm run docker:tag",
    "docker:tag": "cross-conf-env docker image tag $npm_package_config_imageRepo:$npm_package_version $npm_package_config_imageRepo:latest",
    "docker:run": "run-s -c docker:clean docker:runHelper",
    "docker:runHelper": "cross-conf-env docker run --env-file .env --name $npm_package_config_imageName -d -p $npm_package_config_imagePort:$npm_package_config_internalContainerPort $npm_package_config_imageRepo",
    "predocker:publish": "echo Attention! Ensure `docker login` is correct.",
    "docker:publish": "cross-conf-env docker image push $npm_package_config_imageRepo:$npm_package_version",
    "postdocker:publish": "cross-conf-env docker image push $npm_package_config_imageRepo:latest",
    "docker:clean": "cross-conf-env docker rm -f $npm_package_config_imageName",
    "predocker:taillogs": "echo Web Server Logs:",
    "docker:taillogs": "cross-conf-env docker logs -f $npm_package_config_imageName",
    "docker:debugmessage": "echo Docker Debug Completed Successfully! Hit Ctrl+C to terminate log tailing.",
    "prepublishOnly": "npm run test",
    "init:env": "init-dev-env generate-dot-env example.env -f",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abcd"
  },
  "keywords": [
    "typescript",
    "node",
    "mongo"
  ],
  "author": "Asif Javed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abcd/issues"
  },
  "homepage": "https://github.com/streamvault/abcd#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.10",
    "@types/request-ip": "^0.0.38",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.8",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongodb": "^5.7.0",
    "mongoose": "^7.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prettier": "^3.0.0",
    "request-ip": "^3.3.0",
    "rimraf": "^5.0.1",
    "typescript": "4.9.5",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "cross-conf-env": "^1.3.0",
    "dev-norms": "^2.0.0",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "init-dev-env": "^1.0.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  }
}
